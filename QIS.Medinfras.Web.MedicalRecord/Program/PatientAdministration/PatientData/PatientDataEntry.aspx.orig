                    if (result != null) {
                        $('#<%=hdnZipCode.ClientID %>').val(result.ID);
                        $('#<%=txtCity.ClientID %>').val(result.City);
                        $('#<%=txtCounty.ClientID %>').val(result.County);
                        $('#<%=txtDistrict.ClientID %>').val(result.District);
                        $('#<%=txtProvinceCode.ClientID %>').val(result.GCProvince.split('^')[1]);

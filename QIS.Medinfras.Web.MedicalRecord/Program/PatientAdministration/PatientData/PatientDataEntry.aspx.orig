            //#endregion

            //#region Patient Job
            function onGetSCPatientJobFilterExpression() {
                var filterExpression = "<%:OnGetSCPatientJobFilterExpression() %>";
                return filterExpression;
            }

            $('#lblPatientJob.lblLink').click(function () {
